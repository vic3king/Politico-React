    
{
    "env": {
        "node": true,
        "jest": true
    },
  "parser": "babel-eslint",
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      "no-unused-vars": 0,
      "prettier/prettier": [
          "error",
          {
            "trailingComma": "es5",
            "tabWidth": 2,
            "semi": true,
            "singleQuote": true,
            "endOfLine": "lf"
          }
        ]
  },
  "globals": {
      "window": true,
      "document": true,
      "localStorage": true,
      "FormData": true,
      "FileReader": true,
      "Blob": true,
      "navigator": true
    },
  "extends": ["airbnb", "prettier"]
}